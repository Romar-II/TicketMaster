@model TicketMaster.Models.Ticket

@{
    ViewData["Title"] = "Ticket details";
}



<div>
    <h4>Ticket details</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Ticket description
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.TicketBody)
        </dd>
        <dt class = "col-sm-2">
            Ticket due time
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.TicketTimeToFinish)
        </dd>
        <dt class = "col-sm-2">
            Ticket submission time
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.TicketCreationTime)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Id">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
